@{
    ViewBag.ListMods
    ViewBag.ListMaps
}
<div>
    <ul class="list-group">
        <li class="list-group-item">----------МОДЫ----------</li>
        @foreach(var mod in (List<ModEntity>)ViewBag.ListMods) {
            <li class="list-group-item active">@mod</li>
        }
        
        <li class="list-group-item">----------КАРТЫ----------</li>
        @foreach(var map in (List<MapEntity>)ViewBag.ListMaps) {
            <li class="list-group-item active">@map</li>
        }
    </ul>
</div>
<div class="btn-group" role="group">
    <button type="button" class="btn btn-default">Добавить</button>
    <button type="button" class="btn btn-default">Изменить</button>
    <button type="button" class="btn btn-default">Удалить</button>
</div>
<div class="alert alert-warning">Не выбран файл!</div>